    <section class="award-section spad">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <div class="section-title">
                        <h2>Our Awards</h2>
                    </div>
                </div>
            </div>
            <div class="row">
                <% for (const award of awards) { %>
                    <div class="col-lg-4 col-sm-6">
                        <div class="award-item">
                            <img src="<%= award.photo %>" alt="">
                            <h5>
                                <%= award.year %>
                            </h5>
                            <h4>
                                <%= award.name %>
                            </h4>
                            <span>
                                <%= award.value %>
                            </span>
                            <p>
                                <%= award.description %>
                            </p>
                        </div>
                    </div>
                    <% } %>

            </div>
        </div>
    </section>
